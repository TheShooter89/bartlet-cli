{
  "name": "bartlet-cli",
  "version": "0.1.3-0",
  "description": "bartlet-cli CLI",
  "private": false,
  "bin": {
    "bartlet-cli": "bin/bartlet-cli"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "count": "cloc ./",
    "postinstall": "bash postinstall.sh"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "esm": "^3.2.25",
    "gluegun": "latest",
    "inquirer": "^7.0.0",
    "inquirer-file-tree-selection-prompt": "^1.0.3",
    "omelette": "^0.4.12"
  },
  "devDependencies": {
    "cloc": "^2.5.0",
    "jest": "^24.1.0",
    "prettier": "^1.12.1",
    "standard": "^12.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
